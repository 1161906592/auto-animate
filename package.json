{
  "name": "@liuyang0826/auto-animate",
  "type": "module",
  "version": "0.0.1",
  "description": "auto animate when dom change",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "release": "bumpp && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.cjs"
